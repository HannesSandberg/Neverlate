<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:id="@+id/menu_frame">

    <FrameLayout android:id="@+id/list_placeholder" android:layout_weight="1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:lay
        />

    <ImageButton
        android:layout_width="150dip"
        android:layout_height="90dip"
        android:id="@+id/walkButton"

        android:layout_below="@+id/list_placeholder"
        android:background="@drawable/walk"/>

    <ImageButton
        android:layout_width="150dip"
        android:layout_height="90dip"
        android:id="@+id/imageButton"
        android:layout_below="@+id/list_placeholder"
        android:layout_toEndOf="@+id/walkButton"

        android:background="@drawable/bike"/>




</RelativeLayout>